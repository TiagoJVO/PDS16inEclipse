//LDI
	ldi r1, #0x2f

//LDIH
	ldih r1, #0x2f

//LD direct
	//ldb r1, var1
	ldb r1, 0x25
	//ld r3, var1
	ld r3, 0x25

//LD indexed
	ld r0, [r7, #5]
	ldb r1, [r2, #3]
	ldb r3,[r1,#0x0]

//LD based indexed
	ld r0, [r1, r2]
	ldb r2,[r0, r1]

//ADD registers

//ADD registers with CY flag

//ADD constant

//ADD constant with CY flag

